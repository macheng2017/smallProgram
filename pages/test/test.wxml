<view>{{name}}</view>
<button bindtap="changename" >click me!</button>


<view wx:for="{{array}}">
    {{index}}:{{item.message}}
</view>
<view wx:for="{{array}}" wx:for-index="i" wx:for-item="itemName">
    {{i}}:{{itemName.message}}
</view>

<view wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:for-item="i">
    <view wx:for="{{[1,2,3,4,5,6,7,8,9]}}"
    wx:for-item="j">
        <view wx:if="{{i<=j}}">
            {{i}} * {{j}} = {{i*j}}       
        </view>
    </view>
</view>

